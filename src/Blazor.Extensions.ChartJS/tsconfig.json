{
  "compilerOptions": {
    "noImplicitAny": true,
    "noEmitOnError": true,
    "removeComments": true,
    "sourceMap": false,
    "target": "es6",
    "strict": true,
    "alwaysStrict": true,
    "preserveConstEnums": true,
    "outDir": "wwwroot/"
  },
  "include": [
    "Client/*"
  ],
  "compileOnSave": true
}